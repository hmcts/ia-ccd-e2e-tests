                "TEST_E2E_URL_WEB": "https://xui-ia-case-api-pr-2418.preview.platform.hmcts.net/",
                "TEST_ENV": "AAT",
                "TEST_E2E_NUM_BROWSERS": 4,
                "TEST_E2E_HEADLESS" : "false",
                "RETRIES": "0",
                "IDAM_API_URL": "https://idam-api.aat.platform.hmcts.net",
