{
  "name": "ia-ccd-e2e-tests",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "webdriver-manager update --versions.gecko=v0.19.1",
    "lint": "tslint -c tslint.json -p tsconfig.json",
    "e2e": "yarn localTest",
    "test:fullfunctional": "npx @puppeteer/browsers install firefox; node e2e/cucumberTagSearcher.js @testy-test; TESTS_FOR_ACCESSIBILITY='true'; NODE_OPTIONS='--max-old-space-size=8192'; protractor e2e/features.firefoxParallel.conf.js --cucumberOpts.tags=@testy-test --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled",
    "test:localfullfunctional": "protractor ./e2e/features.local.conf.js --parallel-features --cucumberOpts.tags=@testy-test --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled",
    "localTest": "protractor ./e2e/features.conf.js --cucumberOpts.tags=~@toggled-off --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled --cucumberOpts.format='node_modules/cucumber-pretty'",
    "localTestParallelScenarios": "protractor ./e2e/features.parallel.conf.js --parallel-scenarios --cucumberOpts.tags=~@toggled-off --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled --cucumberOpts.format='node_modules/cucumber-pretty'",
    "localTestParallelFeatures": "protractor ./e2e/features.parallel.conf.js --parallel-features --cucumberOpts.tags=~@toggled-off --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled --cucumberOpts.format='node_modules/cucumber-pretty'",
    "test:crossbrowser": "protractor ./e2e/features.crossbrowser.conf.js --parallel-scenarios --cucumberOpts.tags=~@toggled-off --cucumberOpts.tags='@xbrowser' --cucumberOpts.tags=~@OnlyIfSaveAndContinueIsEnabled",
    "test:nsp": "nsp check"
  },
  "pre-commit": [
    "lint"
  ],
  "dependencies": {
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^11.0.5",
    "@axe-core/webdriverjs": "^4.1.1",
    "@hmcts/eslint-config": "^1.4.0",
    "@types/axe-core": "^2.0.7",
    "@types/chai": "^4.1.6",
    "@types/cucumber": "^4.0.4",
    "@types/jasmine": "^2.8.9",
    "@types/node": "^10.12.0",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "codelyzer": "^4.5.0",
    "cucumber": "^6.0.0",
    "cucumber-pretty": "^6.0.0",
    "jasmine-core": "^3.2.1",
    "jasmine-spec-reporter": "^4.2.1",
    "json-schema": "^0.4.0",
    "moment": "^2.29.2",
    "nsp": "^3.2.1",
    "pre-commit": "^1.2.2",
    "protractor": "^5.4.2",
    "protractor-beautiful-reporter": "^1.2.5",
    "protractor-cucumber-framework": "^6.2.1",
    "protractor-multiple-cucumber-html-reporter-plugin": "1.8.1",
    "protractor-retry": "^1.2.13",
    "puppeteer": "1.15.0",
    "request-promise": "^4.2.5",
    "rxjs": "^6.5.4",
    "ts-loader": "^5.2.2",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.3",
    "uuid": "^7.0.0"
  },
  "resolutions": {
    "https-proxy-agent": "^2.2.3",
    "js-yaml": "^3.13.1",
    "json-schema": "^0.4.0",
    "kind-of": "^6.0.3",
    "loader-utils": "^1.4.2",
    "lodash": "^4.17.19",
    "mem": "^4.0.0",
    "minimatch": "^3.0.5",
    "minimist": "^1.2.3",
    "mixin-deep": "^2.0.1",
    "semver": "^5.7.2",
    "set-value": "^4.0.1",
    "yargs-parser": "^18.1.2",
    "yargs": "^15.0.0",
    "xml2js": "^0.6.2",
    "es5-ext": "^0.10.63"
  }
}
